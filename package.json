{
  "name": "inventoryhelper",
  "version": "0.0.2",
  "description": "An inventory helper application for coffee roasting facilities",
  "author": "Beau Davenport <davenportbeau@gmail.com>",
  "private": true,
  "scripts": {
    "start": "node ./lib/bin/www",
    "test": "set jwtString='foo' & mocha --compilers js:babel-core/register __tests__ --recursive --timeout=15000",
    "test:api": "set jwtString='foo' & mocha --compilers js:babel-core/register __tests__/api --recursive --timeout=15000",
    "test:client": "mocha --compilers js:babel-core/register __tests__/client --recursive --timeout=15000",
    "build:api": "babel src/api --watch --out-dir lib --copy-files",
    "build:client": "webpack --watch --progress --colors"
  },
  "dependencies": {
    "async": "^0.9.0",
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "~1.0.0",
    "bson": "^0.2.15",
    "cookie-parser": "~1.0.1",
    "debug": "~0.7.4",
    "defaultcss": "^1.1.1",
    "express": "~4.2.0",
    "express-generator": "^4.9.0",
    "font-awesome": "^4.7.0",
    "jade": "~1.3.0",
    "jwt-simple": "^0.2.0",
    "moment": "^2.8.3",
    "mongodb": "*",
    "mongoskin": "*",
    "monk": "^3.1.3",
    "morgan": "~1.0.0",
    "normalize.css": "^7.0.0",
    "react-router": "^4.1.1",
    "react-router-dom": "^4.1.1",
    "redux-thunk": "^2.2.0",
    "skeleton.css": "^2.0.4",
    "static-favicon": "~1.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.11.4",

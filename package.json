    "babel-preset-es2015": "^6.13.2",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "css-loader": "^0.28.3",
    "enzyme": "^2.8.2",
    "eslint": "^3.12.2",
    "eslint-plugin-mocha": "^4.8.0",
    "eslint-plugin-react": "^6.0.0",
    "extract-text-webpack-plugin": "^2.1.0",
    "fetch-mock": "^5.10.0",
    "file-loader": "^0.11.2",
    "guid": "0.0.12",
